{
  "name": "pierrebilac/shared-bundle",
  "description": "A reusable Symfony bundle for common functionality",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "chiqui3d",
      "homepage": "https://github.com/ranky/shared-bundle",
      "email": "josecarlos@clicksolution.es"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ranky\\SharedBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ranky\\SharedBundle\\Tests\\": "tests/src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^5.4 || ^6.0 ||^7.1",
    "symfony/validator": "^5.4 || ^6.0 ||^7.1",
    "symfony/uid": "^5.4 || ^6.0 ||^7.1",
    "symfony/twig-bundle": "^5.4 || ^6.0 ||^7.1",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/orm": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.4 || ^6.0 ||^7.1",
    "symfony/dotenv": "^5.4 || ^6.0 ||^7.1",
    "symfony/http-kernel": "^5.4 || ^6.0 ||^7.1",
    "symfony/security-bundle": "^5.4 || ^6.0 ||^7.1",
    "symfony/mime": "^5.4 || ^6.0 ||^7.1",
    "psr/log": "^1.0 | ^2.0",
    "dg/bypass-finals": "^1.4",
    "fakerphp/faker": "^1.20",
    "friends-of-behat/mink-extension": "^2.7",
    "friends-of-behat/mink-browserkit-driver": "^1.6",
    "friends-of-behat/symfony-extension": "^2.3",
    "mtdowling/jmespath.php": "^2.6",
    "doctrine/doctrine-fixtures-bundle": "^3.4"
  }
}
